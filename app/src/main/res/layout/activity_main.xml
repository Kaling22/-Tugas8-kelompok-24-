<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".MainActivity">

    <RelativeLayout
        android:id="@+id/rel1"
        android:layout_width="match_parent"
        android:layout_marginRight="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="20dp"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/judul"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Data Vaksinasi Indonesia"
            android:textStyle="bold"
            android:textSize="25dp"
            android:textColor="@color/primary"
            android:layout_centerHorizontal="true"/>
        <androidx.cardview.widget.CardView
            android:id="@+id/card1"
            android:layout_width="200dp"
            android:layout_height="150dp"
            android:layout_below="@+id/judul"
            android:layout_marginTop="20dp"
            app:cardBackgroundColor="@color/card1"
            app:cardCornerRadius="12dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="6dp">

             <TextView
                 android:id="@+id/sasaran"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="Total Sasaran Vaksinasi Indonesia"
                 android:textColor="@color/white"
                 android:textStyle="bold"
                 android:textSize="16dp"
                 android:gravity="center"/>
             <TextView
                 android:id="@+id/sasaranumum"
                 android:layout_width="match_parent"
                 android:layout_height="wrap_content"
                 android:text="123123123"
                 android:gravity="center"
                 android:textSize="20dp"
                 android:textColor="@color/white"
                 android:layout_marginTop="25dp"/>


            </LinearLayout>

        </androidx.cardview.widget.CardView>
        <androidx.cardview.widget.CardView
            android:id="@+id/card2"
            android:layout_width="200dp"
            android:layout_height="150dp"
            android:layout_below="@+id/judul"
            android:layout_toRightOf="@id/card1"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            app:cardBackgroundColor="@color/card2"
            app:cardCornerRadius="12dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="6dp">

                <TextView
                    android:id="@+id/sasarannakes"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Total Sasaran Nakes"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:textSize="16dp"
                    android:gravity="center"/>
                <TextView
                    android:id="@+id/totalnakes"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="123123123"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textColor="@color/white"
                    android:layout_marginTop="25dp"/>


            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/card3"
            android:layout_width="200dp"
            android:layout_height="150dp"
            android:layout_below="@+id/card1"
            android:layout_marginTop="20dp"
            app:cardBackgroundColor="@color/card3"
            app:cardCornerRadius="12dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="6dp">

                <TextView
                    android:id="@+id/lansia"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Total Sasaran Vaksin Lansia"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:textSize="16dp"
                    android:gravity="center"/>
                <TextView
                    android:id="@+id/totallansia"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="123123123"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textColor="@color/white"
                    android:layout_marginTop="25dp"/>


            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/card4"
            android:layout_width="200dp"
            android:layout_height="150dp"
            android:layout_below="@id/card2"
            android:layout_toRightOf="@id/card3"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            app:cardBackgroundColor="@color/card4"
            app:cardCornerRadius="12dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="6dp">

                <TextView
                    android:id="@+id/petugaspublik"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Total Sasaran Petugas Publik"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:textSize="16dp"
                    android:gravity="center"/>
                <TextView
                    android:id="@+id/totalpublik"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="123123123"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textColor="@color/white"
                    android:layout_marginTop="25dp"/>


            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/card5"
            android:layout_width="200dp"
            android:layout_height="150dp"
            android:layout_below="@+id/card3"
            android:layout_marginTop="20dp"
            app:cardBackgroundColor="@color/card5"
            app:cardCornerRadius="12dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="6dp">

                <TextView
                    android:id="@+id/lansia"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Total Vaksin Dosis 1 Indonesia"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:textSize="16dp"
                    android:gravity="center"/>
                <TextView
                    android:id="@+id/vak1"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="123123123"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textColor="@color/white"
                    android:layout_marginTop="25dp"/>


            </LinearLayout>

        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/card6"
            android:layout_width="200dp"
            android:layout_height="150dp"
            android:layout_below="@id/card3"
            android:layout_toRightOf="@id/card5"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            app:cardBackgroundColor="@color/card6"
            app:cardCornerRadius="12dp">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="8dp"
                android:layout_marginLeft="8dp"
                android:layout_marginRight="6dp">

                <TextView
                    android:id="@+id/petugaspublik"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Total Vaksin Dosis 2 Indonesia"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:textSize="16dp"
                    android:gravity="center"/>
                <TextView
                    android:id="@+id/vak2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="123123123"
                    android:gravity="center"
                    android:textSize="20dp"
                    android:textColor="@color/white"
                    android:layout_marginTop="25dp"/>
            </LinearLayout>
        </androidx.cardview.widget.CardView>

    </RelativeLayout>
</RelativeLayout>
